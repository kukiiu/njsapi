{
    "user": "nginx",
    "worker_processes": "auto",
    "pid": "/var/run/nginx.pid",
    "worker_connections": 1024,
    "access_log": "/var/log/nginx/access.log",
    "access_log_format": "'$remote_addr - $remote_user [$time_local] \"$request\" ' '$status $body_bytes_sent \"$http_referer\" ' '\"$http_user_agent\" \"$http_x_forwarded_for\"'",
    "error_log": "/var/log/nginx/error.log",
    "error_log_level": "",
    "js_path": "\"/usr/local/njsapi/src/\"",
    "listen": 80
}